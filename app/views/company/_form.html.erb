<%= form_for (@company) do |c| %>
<form class="form-group">
	<div class="form-group form-control">
		<%= c.label 'Aliado Comercial' %><br>
		<%= c.text_field :name_company, label: "Aliado Comercial" %>
	</div>
	<div class="form-group form-control">
		<%= c.label 'Interventor' %><br>
		<%= c.text_field :controller%>
	</div>

	<div class="form-group form-control">
		<%= c.label 'Teléfono' %><br>
		<%= c.number_field :phone %>	
	</div>
	<div class="form-group form-control">
		<%= c.label 'Correo Electrónico' %><br>
		<%= c.email_field :email %>
	</div>

	<div class="form-group form-control">	
		<%= c.label 'Cantidad de Sedes' %><br>
		<%= c.number_field :quantityoffice %>
	</div>
	<div class="actions">
		<%= c.submit 'Guardar'%>
	</div>
</form>
	

<% end %>